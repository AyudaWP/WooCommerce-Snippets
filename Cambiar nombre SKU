
//Cambiar texto SKU a otra cosa - este c√≥digo en tu functions.php
function translate_woocommerce($translation, $text, $domain) {
    if ($domain == 'woocommerce') {
        switch ($text) {
            case 'SKU':
                $translation = 'ISBN';
                break;
            case 'SKU:':
                $translation = 'ISBN:';
                break;
        }
    }
    return $translation;
}

add_filter('gettext', 'translate_woocommerce', 10, 3);
